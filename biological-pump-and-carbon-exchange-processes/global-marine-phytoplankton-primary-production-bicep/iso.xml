          <gmd:title>
            <gco:CharacterString>Monthly global Marine Phytoplankton Primary Production dataset</gco:CharacterString>
          </gmd:title>
          <gmd:date>
            <gmd:CI_Date>
              <gmd:date>
                <gco:Date>2021-10-01</gco:Date>
              </gmd:date>
              <gmd:dateType>
